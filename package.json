{
  "name": "catering-reservation-api",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node -r tsconfig-paths/register dist/app.js",
    "build": "tsc",
    "check": "tsc --noEmit",
    "dev": "tsx watch app.ts",
    "build:dev": "tsc && node dist/app.js"
  },
  "_moduleAliases": {
    "@controllers": "/dist/src/controllers",
    "@database": "/dist/src/database",
    "@middlewares": "/dist/src/middlewares",
    "@models": "/dist/src/models",
    "@routes": "/dist/src/routes",
    "@schemas": "/dist/src/schemas",
    "@config": "/dist/src/config",
    "@utils/*": "/dist/src/utils",
    "@TStypes": "/dist/src/typescript/types"
  },
  "dependencies": {
    "@faker-js/faker": "^9.5.1",
    "bcryptjs": "^3.0.2",
    "cloudinary": "^2.6.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.4.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongodb": "^6.15.0",
    "mongoose": "^8.12.2",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "socket.io": "^4.8.1",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.13.10",
    "eslint": "^9.21.0",
    "globals": "^16.0.0",
    "nodemon": "^3.1.9",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
